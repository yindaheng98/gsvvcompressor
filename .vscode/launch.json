{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gsvvcompressor: encode",
            "type": "debugpy",
            "request": "launch",
            "module": "gsvvcompressor",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "encode",
                "vqxyzdraco2pass",
                "input.first_frame_path=output/walking/frame1/point_cloud/iteration_10000/point_cloud.ply",
                "input.max_frames=29",
                "input.subsequent_format='output/walking/refine/masked-propagate-dot-cotracker3/masked-propagate-dot-cotracker3/frame{:d}/point_cloud/iteration_1000/point_cloud.ply'",
                "output.path=output/walking/refine/masked-propagate-dot-cotracker3.bin",
                "output.overwrite=true",
                "codec.vq.num_clusters_scaling=1024",
                "codec.vq.num_clusters_rotation_re=256",
                "codec.vq.num_clusters_rotation_im=1024",
                "codec.vq.num_clusters_opacity=256",
                "codec.vq.num_clusters_features_dc=512",
                "codec.vq.num_clusters_features_rest=[256,128,64]",
            ]
        },
    ]
}